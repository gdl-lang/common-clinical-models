          }
        }
      }
    },
    "rules": {
      "gt0015": {
        "id": "gt0015",
        "priority": 9,
        "when": [
          "$gt0003|Unit system|==local::at0005|English|",
          "$gt0004|Body fat|!=null",
          "$gt0016|Height (feet)|!=null",
          "$gt0006|Height|.unit=='[in_i]'",
          "$gt0008|Weight|.unit=='[lb_av]'"
        ],
        "then": [
          "$gt0010|Lean|.magnitude=$gt0008.magnitude*(1-($gt0004.magnitude/100))",
          "$gt0010|Lean|.precision=2",
          "$gt0010|Lean|.unit='[lbs]'",
          "$gt0011|FFMI|.magnitude=($gt0010.magnitude*0.45359237)*(((($gt0016.magnitude*12)+$gt0006.magnitude)*0.0254)^(0-2))",
          "$gt0011|FFMI|.precision=2",
          "$gt0011|FFMI|.unit='1'",
          "$gt0012|Adjusted FFMI|.magnitude=$gt0011.magnitude+6.1*(1.8-($gt0016.magnitude*12.0+$gt0006.magnitude)*0.0254)",
          "$gt0012|Adjusted FFMI|.precision=2",
          "$gt0012|Adjusted FFMI|.unit='1'"
        ]
      },
      "gt0019": {
        "id": "gt0019",
        "priority": 8,
        "when": [
          "$gt0003|Unit system|==local::at0006|Metric|",
          "$gt0004|Body fat|!=null",
          "$gt0006|Height|.unit=='cm'",
          "$gt0008|Weight|.unit=='kg'",
          "$gt0016|Height (feet)|==null"
        ],
        "then": [
          "$gt0010|Lean|.magnitude=$gt0008.magnitude*(1-($gt0004.magnitude/100))",
          "$gt0010|Lean|.precision=2",
          "$gt0010|Lean|.unit='kg'",
          "$gt0011|FFMI|.magnitude=$gt0010.magnitude*(($gt0006.magnitude*0.01)^(0-2))",
          "$gt0011|FFMI|.precision=2",
          "$gt0011|FFMI|.unit='1'",
          "$gt0012|Adjusted FFMI|.magnitude=$gt0011.magnitude+6.1*(1.8-($gt0006.magnitude*0.01))",
          "$gt0012|Adjusted FFMI|.precision=2",
          "$gt0012|Adjusted FFMI|.unit='1'"
        ]
      },
      "gt0020": {
        "id": "gt0020",
        "priority": 7,
        "when": [
          "$gt0011|FFMI|>=16,1",
          "$gt0011|FFMI|<=17,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Below average'"
        ]
      },
      "gt0021": {
        "id": "gt0021",
        "priority": 6,
        "when": [
          "$gt0011|FFMI|>=18,1",
          "$gt0011|FFMI|<=19,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Average'"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 5,
        "when": [
          "$gt0011|FFMI|>=20,1",
          "$gt0011|FFMI|<=21,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Above average'"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 4,
        "when": [
          "$gt0011|FFMI|==22,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Excellent'"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 3,
        "when": [
          "$gt0011|FFMI|>=23,1",
          "$gt0011|FFMI|<=25,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Superior'"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 2,
        "when": [
          "$gt0011|FFMI|>=26,1",
          "$gt0011|FFMI|<=27,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Suspicion of steroid use'"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 1,
        "when": [
          "$gt0011|FFMI|>=28,1",
          "$gt0011|FFMI|<=30,1"
        ],
        "then": [
          "$gt0014|Interpretation|='Steroid usage likely'"
        ]
      }
    }
  },
