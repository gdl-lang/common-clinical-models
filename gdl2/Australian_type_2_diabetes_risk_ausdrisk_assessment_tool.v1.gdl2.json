          "$gt0020|BMI|=8|local::at0046|Morbidly obese (≥35)|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 12,
        "when": [
          "$gt0016|Body Mass Index|>=30,kg/m2",
          "$gt0016|Body Mass Index|<35,kg/m2"
        ],
        "then": [
          "$gt0020|BMI|=6|local::at0045|Obese (30 to less than 35)|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 11,
        "when": [
          "$gt0016|Body Mass Index|>=25,kg/m2",
          "$gt0016|Body Mass Index|<30,kg/m2"
        ],
        "then": [
          "$gt0020|BMI|=3|local::at0044|Overweight (25 to less than 30)|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 10,
        "when": [
          "$gt0016|Body Mass Index|<25,kg/m2"
        ],
        "then": [
          "$gt0020|BMI|=0|local::at0043|Normal (<25)|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 9,
        "when": [
          "$gt0013|Obesity parameter|==local::at0054|Waist circumference (recommended)|",
          "$gt0014|Waist circumference|!=null",
          "$gt0006|Gender|!=null",
          "$gt0007|Ethnicity|!=null",
          "$gt0008|Parental history of diabetes|!=null",
          "$gt0009|History of high blood glucose|!=null",
          "$gt0010|Use of antihypertensive medications|!=null",
          "$gt0011|Current smoker|!=null",
          "$gt0012|Physical inactivity|!=null",
          "$gt0019|Age (years)|!=null"
        ],
        "then": [
          "$gt0025|Total score|.magnitude=$gt0014.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0019.value"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 8,
        "when": [
          "$gt0013|Obesity parameter|==local::at0055|BMI|",
          "$gt0016|Body Mass Index|!=null",
          "$gt0006|Gender|!=null",
          "$gt0007|Ethnicity|!=null",
          "$gt0008|Parental history of diabetes|!=null",
          "$gt0009|History of high blood glucose|!=null",
          "$gt0010|Use of antihypertensive medications|!=null",
          "$gt0011|Current smoker|!=null",
          "$gt0012|Physical inactivity|!=null"
        ],
        "then": [
          "$gt0025|Total score|.magnitude=$gt0006.value+$gt0020.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0019.value"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 7,
        "when": [
          "$gt0013|Obesity parameter|==local::at0056|Both|",
          "$gt0014|Waist circumference|!=null",
          "$gt0016|Body Mass Index|!=null"
        ],
        "then": [
          "$gt0025|Total score|.magnitude=$gt0020.value+$gt0014.value+$gt0006.value+$gt0007.value+$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0019.value"
        ]
