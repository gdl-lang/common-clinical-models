    "pre_conditions": [
      "$gt0003|Alcoholic hepatitis|==1|local::at0016|Yes|",
      "$gt0004|Is the patient being treated with steroid?|==1|local::at0018|Yes|"
    ],
    "rules": {
      "gt0026": {
        "id": "gt0026",
        "priority": 15,
        "when": [
          "$gt0006|Birthdate|!=null"
        ],
        "then": [
          "$gt0014|Age|.magnitude=($currentDateTime-$gt0006)/1,a",
          "$gt0014|Age|.precision=0",
          "$gt0014|Age|.unit='a'"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 14,
        "when": [
          "$gt0041|Initial bilirubin|.unit=='mg/dl'"
        ],
        "then": [
          "$gt0015|Initial Bilirubin|.magnitude=$gt0041.magnitude*17.1",
          "$gt0015|Initial Bilirubin|.precision=2",
          "$gt0015|Initial Bilirubin|.unit='umol/l'"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 13,
        "when": [
          "$gt0041|Initial bilirubin|.unit=='µmol/l'"
        ],
        "then": [
          "$gt0015|Initial Bilirubin|=$gt0041|Initial bilirubin|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 12,
        "when": [
          "$gt0009|Bilirubin day-7|.unit=='mg/dl'"
        ],
        "then": [
          "$gt0016|Bilirubin day-7|.magnitude=$gt0009.magnitude*17.1",
          "$gt0016|Bilirubin day-7|.precision=2",
          "$gt0016|Bilirubin day-7|.unit='umol/l'"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 11,
        "when": [
          "$gt0009|Bilirubin day-7|.unit=='umol/l'"
        ],
        "then": [
          "$gt0016|Bilirubin day-7|=$gt0009|Bilirubin day-7|"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 10,
        "when": [
          "$gt0048|Albumin|!=null"
        ],
        "then": [
          "$gt0017|Albumin|=$gt0048|Albumin|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 9,
        "when": [
          "$gt0050|Creatinine|.unit=='mg/dl'",
          "$gt0050|Creatinine|.magnitude<=1.3"
        ],
        "then": [
          "$gt0018|Renal Insuficiency|=0|local::at0008|Cr ≤1.3 mg/dL or 115 µmol/L)|"
        ]
      },
      "gt0051": {
        "id": "gt0051",
        "priority": 8,
        "when": [
          "$gt0050|Creatinine|.unit=='µmol/l'",
          "$gt0050|Creatinine|.magnitude<=115"
        ],
        "then": [
          "$gt0018|Renal Insuficiency|=0|local::at0008|Cr ≤1.3 mg/dL or 115 µmol/L)|"
        ]
      },
      "gt0052": {
        "id": "gt0052",
        "priority": 7,
        "when": [
          "$gt0050|Creatinine|.unit=='mg/dl'",
          "$gt0050|Creatinine|.magnitude>1.3"
        ],
        "then": [
          "$gt0018|Renal Insuficiency|=1|local::at0009| Cr >1.3 mg/dL or 115 µmol/L|"
        ]
      },
      "gt0053": {
        "id": "gt0053",
        "priority": 6,
        "when": [
          "$gt0050|Creatinine|.unit=='µmol/l'",
          "$gt0050|Creatinine|.magnitude>115"
        ],
        "then": [
          "$gt0018|Renal Insuficiency|=1|local::at0009| Cr >1.3 mg/dL or 115 µmol/L|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 5,
        "when": [
          "$gt0044|PT|!=null"
        ],
        "then": [
          "$gt0019|PT|=$gt0044|PT|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 4,
        "when": [
          "$gt0014|Age|!=null",
          "$gt0041|Initial bilirubin|!=null",
          "$gt0009|Bilirubin day-7|!=null",
          "$gt0048|Albumin|!=null",
          "$gt0018|Renal Insuficiency|!=null",
          "$gt0044|PT|!=null"
        ],
        "then": [
          "$gt0045|R|.magnitude=3.19-(0.101*$gt0014.magnitude)+(0.147*$gt0017.magnitude)+(0.0165*($gt0015.magnitude-$gt0016.magnitude))-(0.206*$gt0018.value)-(0.0065*$gt0015.magnitude)-(0.0096*$gt0019.magnitude)",
          "$gt0045|R|.precision=4"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 3,
        "when": [
          "$gt0045|R|!=null"
        ],
        "then": [
          "$gt0046|Lille Model Score|.magnitude=(1/(2.71828183^$gt0045.magnitude))/(1+(1/(2.71828183^$gt0045.magnitude)))",
          "$gt0046|Lille Model Score|.precision=3"
        ]
      },
      "gt0038": {
        "id": "gt0038",
        "priority": 2,
        "when": [
          "$gt0046|Lille Model Score|<'0.45'"
        ],
        "then": [
          "$gt0023|Prognosis|=1|local::at0009|Good prognosis.|",
          "$gt0024|6-month survival rate|=1|local::at0006|6-month survival rate of 85%.|",
          "$gt0025|Recommendation|=1|local::at0012|The patient might respond to steroid therapy. Consider to continue the treatment.|"
        ]
      },
      "gt0039": {
        "id": "gt0039",
        "priority": 1,
        "when": [
          "$gt0046|Lille Model Score|>'0.45'"
        ],
        "then": [
          "$gt0023|Prognosis|=0|local::at0008|Poor prognosis.|",
          "$gt0024|6-month survival rate|=0|local::at0005|6-month survival rate of 25%.|",
          "$gt0025|Recommendation|=0|local::at0011|The patient may be a non-responder to steroids therapy. Consider alternative therapies and/or referral for liver transplant.|"
        ]
      }
    }
  },
