    "rules": {
      "gt0027": {
        "id": "gt0027",
        "priority": 23,
        "when": [
          "$gt0068|Length of Stay|!=null",
          "$gt0067|Charlson Comorbidity Index|!=null",
          "$gt0005|Admission type|!=null",
          "$gt0060|Number of previous ED visits within 6 months|!=null"
