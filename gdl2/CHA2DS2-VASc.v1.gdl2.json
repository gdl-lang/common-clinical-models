      },
      "gt0034": {
        "id": "gt0034",
        "priority": 11,
        "when": [
          "$gt0016|Gender|!=null",
          "$gt0017|Age|!=null",
          "$gt0011|Congestive Heart Failure|!=null",
          "$gt0012|Hypertension|!=null",
          "$gt0013|Stroke/TIA/Thromboembolism|!=null",
          "$gt0014|Vascular disease|!=null",
          "$gt0015|Diabetes|!=null"
        ],
        "then": [
          "$gt0023|Total score|.magnitude=$gt0011.value+$gt0012.value+$gt0013.value+$gt0014.value+$gt0015.value+$gt0016.value+$gt0017.value"
        ]
      },
      "gt0040": {
        "id": "gt0040",
        "priority": 10,
        "when": [
          "$gt0023|Total score|==0"
        ],
        "then": [
          "$gt0037|Risk assessment|=0|local::at0005|Low risk|"
        ]
      },
      "gt0041": {
