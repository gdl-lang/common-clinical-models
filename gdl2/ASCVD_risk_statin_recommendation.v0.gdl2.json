      "gt0023": {
        "id": "gt0023",
        "priority": 12,
        "when": [
          "$gt0027|Treatment for hypertension|==local::at0033|Yes|",
          "$gt0056|Sex|==local::at0006|Female|",
          "$gt0055|Age|!=null",
          "$gt0029|Diabetes|!=null",
          "$gt0030|Smoker|!=null",
          "$gt0017|HDL Cholesterol|!=null",
          "$gt0018|Total Cholesterol|!=null",
          "$gt0015|Systolic blood pressure|!=null",
          "($gt0026|Race|==local::at0011|White|)||($gt0026|Race|==local::at0013|Other|)",
          "($gt0064|History of ASCVD|==local::at0006|Yes|)||(($gt0065|LDL Cholesterol ≥190mg/dL (4.92 mmol/L)|==local::at0008|No|)&&($gt0064|History of ASCVD|==local::at0005|No|))"
        ],
        "then": [
          "$gt0052|Individual sum|.magnitude=((0-29.799)*log($gt0055.magnitude))+(4.884*log($gt0055.magnitude)^2)+(log($gt0018.magnitude*$gt0019)*13.54)+(log($gt0018.magnitude*$gt0019)*log($gt0055.magnitude)*(0-3.114))+(log($gt0017.magnitude*$gt0020)*(0-13.578))+(log($gt0055.magnitude)*log($gt0017.magnitude*$gt0020)*3.149)+(log($gt0015.magnitude)*2.019)+(7.574*$gt0030.value)+(log($gt0055.magnitude)*$gt0030.value*(0-1.665))+(0.661*$gt0029.value)",
          "$gt0052|Individual sum|.precision=2",
          "$gt0053|Estimated 10-y risk|.magnitude=abs((1-(0.9665^($gt0052.magnitude+29.2)))*100)",
          "$gt0053|Estimated 10-y risk|.precision=1",
          "$gt0053|Estimated 10-y risk|.unit='%'"
