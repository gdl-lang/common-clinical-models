      "gt0013": {
        "id": "gt0013",
        "model_id": "openEHR-EHR-OBSERVATION.basic_demographic.v1",
        "template_id": "openEHR-EHR-OBSERVATION.basic_demographic.v1",
        "type": "INPUT",
        "elements": {
          "gt0014": {
            "id": "gt0014",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0013]"
          }
        }
      },
      "gt0019": {
        "id": "gt0019",
        "model_id": "openEHR-EHR-EVALUATION.fibrosis_4_fib_4_index_for_liver_fibrosis.v0",
        "template_id": "openEHR-EHR-EVALUATION.fibrosis_4_fib_4_index_for_liver_fibrosis.v0",
        "type": "INPUT",
        "elements": {
          "gt0020": {
            "id": "gt0020",
            "path": "/data[at0001]/items[at0002]"
          }
        }
      },
      "gt0025": {
        "id": "gt0025",
        "model_id": "openEHR-EHR-OBSERVATION.mdrd_gfr_equation.v0",
        "template_id": "openEHR-EHR-OBSERVATION.mdrd_gfr_equation.v0",
        "type": "INPUT",
        "elements": {
          "gt0026": {
            "id": "gt0026",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          }
        }
      }
    },
    "rules": {
      "gt0018": {
        "id": "gt0018",
        "priority": 11,
        "when": [
          "$gt0014|Age|>=65,a"
        ],
        "then": [
          "$gt0015|Age|=27|local::at0008|>= 65 years|"
        ]
      },
      "gt0017": {
        "id": "gt0017",
        "priority": 10,
        "when": [
          "$gt0014|Age|>=50,a",
          "$gt0014|Age|<=64,a"
        ],
        "then": [
          "$gt0015|Age|=12|local::at0007|50 - 64 years|"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 9,
        "when": [
          "$gt0014|Age|<50,a"
        ],
        "then": [
          "$gt0015|Age|=0|local::at0006|< 50 years|"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 8,
        "when": [
          "$gt0020|FIB-4 Score|<1.45,1"
        ],
        "then": [
          "$gt0021|FIB-4 Index|=0|local::at0026|< 1.45|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 7,
        "when": [
          "$gt0020|FIB-4 Score|>=1.45,1",
          "$gt0020|FIB-4 Score|<=3.25,1"
        ],
        "then": [
          "$gt0021|FIB-4 Index|=6|local::at0027|1.45 - 3.25|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 6,
        "when": [
          "$gt0020|FIB-4 Score|>3.25,1"
        ],
        "then": [
          "$gt0021|FIB-4 Index|=25|local::at0028|> 3.25|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 5,
        "when": [
          "$gt0026|Glomerular Filtration Rate by MDRD-GFR equation|>=60,ml/min"
        ],
        "then": [
          "$gt0027|eGFR 2|=0|local::at0030|>= 60 mL/min|"
        ]
      },
      "gt0029": {
        "id": "gt0029",
        "priority": 4,
        "when": [
          "$gt0026|Glomerular Filtration Rate by MDRD-GFR equation|>=45,ml/min",
          "$gt0026|Glomerular Filtration Rate by MDRD-GFR equation|<=59.9,ml/min"
        ],
        "then": [
          "$gt0027|eGFR 2|=6|local::at0031|45 - 59.9 mL/min|"
        ]
      },
      "gt0030": {
        "id": "gt0030",
        "priority": 3,
        "when": [
          "$gt0026|Glomerular Filtration Rate by MDRD-GFR equation|>=30,ml/min",
          "$gt0026|Glomerular Filtration Rate by MDRD-GFR equation|<=44.9,ml/min"
        ],
        "then": [
          "$gt0027|eGFR 2|=8|local::at0032|30 - 44.9 mL/min|"
        ]
      },
      "gt0031": {
        "id": "gt0031",
        "priority": 2,
        "when": [
          "$gt0026|Glomerular Filtration Rate by MDRD-GFR equation|<30,ml/min"
        ],
        "then": [
          "$gt0027|eGFR 2|=26|local::at0033|< 30 mL/min|"
        ]
      },
      "gt0012": {
        "id": "gt0012",
        "priority": 1,
        "when": [
          "$gt0015|Age|!=null",
          "$gt0004|CD4 count|!=null",
          "$gt0005|HIV-1 RNA|!=null",
          "$gt0006|Hemoglobin|!=null",
          "$gt0021|FIB-4 Index|!=null",
          "$gt0027|eGFR 2|!=null",
          "$gt0009|Hepatitis C co-infection|!=null"
        ],
        "then": [
          "$gt0011|Total score|.magnitude=$gt0015.value+$gt0004.value+$gt0005.value+$gt0006.value+$gt0021.value+$gt0027.value+$gt0009.value"
        ]
      }
    }
  },
