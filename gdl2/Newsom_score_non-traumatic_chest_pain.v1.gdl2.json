        "when": [
          "($gt0004|Congestive Heart Failure history|==local::at0051|Yes|)||(($gt0005|Smoking history|==local::at0054|Yes|)||(($gt0006|Hemoptysis|==local::at0057|Yes|)||(($gt0007|Tuberculosis history|==local::at0060|Yes|)||(($gt0008|Thromboembolic history|==local::at0063|Yes|)||(($gt0009|Prior/current alcohol abuse|==local::at0066|Yes|)||(($gt0010|Diminished breath sounds|==local::at0079|Yes|)||(($gt0011|Rales|==local::at0081|Yes|)||(($gt0013|Age|>=60,a)||(($gt0015|Respiration rate|>24,/min)||(($gt0019|Oxygen saturation|<90,100,2)||((($gt0017|Body temperature|.magnitude>=38)&&($gt0017|Body temperature|.unit=='Cel'))||(($gt0017|Body temperature|.magnitude>=100.4)&&($gt0017|Body temperature|.unit=='[degF]')))))))))))))"
        ],
