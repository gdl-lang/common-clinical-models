{
  "id": "Blood_Donation_Time_Calculator_guideline.v1",
  "gdl_version": "2.0",
  "concept": "gt0001",
  "language": {
    "original_language": "ISO_639-1::en"
  },
  "description": {
    "original_author": {
      "date": "2021-06-17",
      "name": "Maryam Razavi",
      "organisation": "Cambio CDS",
      "email": "maryam.razavi@cambio.se"
    },
    "lifecycle_state": "Author draft",
    "details": {
      "en": {
        "id": "en",
        "purpose": "To calculate your next possible blood donation date, based on your last blood donation date.",
        "keywords": [
          "donation",
          "blood",
          "time"
        ],
        "use": "Blood donation time:\nTwo subsequent donations have to be 12 weeks (for men) and 16 weeks (for women) apart.",
        "copyright": "Â© Cambio CDS"
      }
    },
    "other_details": {
      "references": "Blood Donor Selection: Guidelines on Assessing Donor Suitability for Blood Donation. Geneva: World Health Organization; 2012. 4, General donor assessment. Available from: https://www.ncbi.nlm.nih.gov/books/NBK138219/"
    }
  },
  "definition": {
    "data_bindings": {
      "gt0008": {
        "id": "gt0008",
        "model_id": "openEHR-EHR-OBSERVATION.blood_donation_time_calculator.v0",
        "template_id": "openEHR-EHR-OBSERVATION.blood_donation_time_calculator.v0",
        "type": "INPUT",
        "elements": {
          "gt0009": {
            "id": "gt0009",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0005]"
          }
        }
      },
      "gt0010": {
        "id": "gt0010",
        "model_id": "openEHR-EHR-OBSERVATION.blood_donation_time_calculator.v0",
        "template_id": "openEHR-EHR-OBSERVATION.blood_donation_time_calculator.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0012": {
            "id": "gt0012",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0006]"
          }
        }
      },
      "gt0020": {
        "id": "gt0020",
        "model_id": "openEHR-EHR-OBSERVATION.basic_demographic.v1",
        "template_id": "openEHR-EHR-OBSERVATION.basic_demographic.v1",
        "type": "INPUT",
        "elements": {
          "gt0021": {
            "id": "gt0021",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0004]"
          }
        }
      }
    },
    "rules": {
      "gt0019": {
        "id": "gt0019",
        "priority": 2,
        "when": [
          "$gt0009|Latest donation|!=null",
          "$gt0021|Gender|==local::at0005|Male|"
        ],
        "then": [
          "$gt0012|Next donation|=$gt0009+12,wk"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 1,
        "when": [
          "$gt0021|Gender|==local::at0006|Female|",
          "$gt0009|Latest donation|!=null"
        ],
        "then": [
          "$gt0012|Next donation|=$gt0009+16,wk"
        ]
      }
    }
  },
  "ontology": {
