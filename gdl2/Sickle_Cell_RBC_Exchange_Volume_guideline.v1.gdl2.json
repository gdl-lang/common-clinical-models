    "default_actions": [
      "$gt0021|Height conversion factor|=1",
      "$gt0023|Weight conversion factor|=1"
    ],
    "rules": {
      "gt0020": {
        "id": "gt0020",
        "priority": 7,
        "when": [
          "$gt0005|Height/Length|.unit=='in'"
        ],
        "then": [
          "$gt0021|Height conversion factor|=2.54"
        ]
      },
      "gt0022": {
        "id": "gt0022",
        "priority": 6,
        "when": [
          "$gt0007|Weight|.unit=='lb'"
        ],
        "then": [
          "$gt0023|Weight conversion factor|=0.45359237"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 5,
        "when": [
          "$gt0003|Gender|==local::at0005|Male|",
          "$gt0005|Height/Length|!=null",
          "$gt0007|Weight|!=null"
        ],
        "then": [
          "$gt0019|Total blood volume|.magnitude=0.3669*($gt0005.magnitude*$gt0021/100)^3+0.03219*($gt0007.magnitude*$gt0023)+0.6041",
          "$gt0019|Total blood volume|.unit='ml'",
          "$gt0019|Total blood volume|.precision=2"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 4,
        "when": [
          "$gt0003|Gender|==local::at0006|Female|",
          "$gt0005|Height/Length|!=null",
          "$gt0007|Weight|!=null"
        ],
        "then": [
          "$gt0019|Total blood volume|.magnitude=0.3561*($gt0005.magnitude*$gt0021/100)^3+0.03308*($gt0007.magnitude*$gt0023)+0.1833",
          "$gt0019|Total blood volume|.unit='ml'",
          "$gt0019|Total blood volume|.precision=2"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 3,
        "when": [
          "$gt0019|Total blood volume|!=null",
          "$gt0009|Patient hematocrit|!=null"
        ],
        "then": [
          "$gt0018|RBC volume|.magnitude=($gt0019.magnitude*1000)*$gt0009.magnitude/100",
          "$gt0018|RBC volume|.unit='ml'",
          "$gt0018|RBC volume|.precision=2"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 2,
        "when": [
          "$gt0018|RBC volume|!=null",
          "$gt0009|Patient hematocrit|!=null",
          "$gt0033|Patient's initial HgbS|!=null",
          "$gt0037|Hematocrit of transfused RBCs|!=null"
        ],
        "then": [
          "$gt0015|RBC replacement volume|.magnitude=($gt0018.magnitude*log($gt0033.magnitude/$gt0035.magnitude)/$gt0037.magnitude)*100",
          "$gt0015|RBC replacement volume|.unit='ml'",
          "$gt0015|RBC replacement volume|.precision=2"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 1,
        "when": [
          "$gt0015|RBC replacement volume|!=null"
        ],
        "then": [
          "$gt0016|Approximate number of units|.magnitude=$gt0015.magnitude/350",
          "$gt0016|Approximate number of units|.precision=0",
          "$gt0016|Approximate number of units|.unit='1'"
        ]
      }
    }
  },
