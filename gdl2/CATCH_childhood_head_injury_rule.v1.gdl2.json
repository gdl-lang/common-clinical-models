    "rules": {
      "gt0022": {
        "id": "gt0022",
        "priority": 7,
        "when": [
          "$gt0003|GCS at 2 hours after injury|<15"
        ],
        "then": [
          "$gt0016|GCS <15 at 2 hours after injury|=1|local::at0007|Yes|"
        ]
      },
      "gt0023": {
        "id": "gt0023",
        "priority": 6,
        "when": [
          "$gt0003|GCS at 2 hours after injury|==15"
        ],
        "then": [
          "$gt0016|GCS <15 at 2 hours after injury|=0|local::at0006|No|"
        ]
      },
      "gt0024": {
        "id": "gt0024",
        "priority": 5,
        "when": [
          "$gt0003|GCS at 2 hours after injury|!=null",
          "$gt0005|Suspected open or depressed skull fracture|!=null",
          "$gt0006|History of worsening headache|!=null",
          "$gt0007|Irritability on examination|!=null"
        ],
        "then": [
          "$gt0017|High risk criteria point|.magnitude=$gt0016.value+$gt0005.value+$gt0006.value+$gt0007.value"
        ]
      },
      "gt0025": {
        "id": "gt0025",
        "priority": 4,
        "when": [
          "$gt0008|Hemotympanum|!=null",
          "$gt0009|Raccoon eyes sign|!=null",
          "$gt0010|Otorrhea|!=null",
          "$gt0011|Rhinorrhea of cerebrospinal fluid|!=null",
          "$gt0012|Battle's sign|!=null",
          "$gt0013|Large, boggy hematoma of the scalp|!=null",
          "$gt0014|Dangerous mechanism of injury|!=null"
        ],
        "then": [
          "$gt0018|Medium risk criteria point|.magnitude=$gt0008.value+$gt0009.value+$gt0010.value+$gt0011.value+$gt0012.value+$gt0013.value+$gt0014.value"
        ]
      },
      "gt0026": {
        "id": "gt0026",
        "priority": 3,
        "when": [
          "$gt0017|High risk criteria point|>=1"
        ],
        "then": [
          "$gt0020|Risk assessment|=2|local::at0006|High risk|",
          "$gt0021|Recommendation|=2|local::at0010|Consider head CT-scan and neurologic intervention|"
        ]
      },
      "gt0027": {
        "id": "gt0027",
        "priority": 2,
        "when": [
          "$gt0017|High risk criteria point|==0",
          "$gt0018|Medium risk criteria point|>=1"
        ],
        "then": [
          "$gt0020|Risk assessment|=1|local::at0004|Medium risk|",
          "$gt0021|Recommendation|=1|local::at0009|Consider head CT-scan|"
        ]
      },
      "gt0028": {
        "id": "gt0028",
        "priority": 1,
        "when": [
          "$gt0017|High risk criteria point|==0",
          "$gt0018|Medium risk criteria point|==0"
        ],
        "then": [
          "$gt0020|Risk assessment|=0|local::at0003|Low risk|",
          "$gt0021|Recommendation|=0|local::at0008|Conservative approach. Head CT-scan is not necessary.|"
        ]
      }
    }
