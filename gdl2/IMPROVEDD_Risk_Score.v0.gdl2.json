      "gt0027": {
        "id": "gt0027",
        "model_id": "openEHR-EHR-OBSERVATION.improvedd_risk_score_.v0",
        "template_id": "openEHR-EHR-OBSERVATION.improvedd_risk_score_.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0028": {
            "id": "gt0028",
            "path": "/data[at0001]/events[at0002]/data[at0003]/items[at0029]"
          }
        }
      },
      "gt0029": {
        "id": "gt0029",
        "model_id": "openEHR-EHR-EVALUATION.improvedd_risk_score_.v0",
        "template_id": "openEHR-EHR-EVALUATION.improvedd_risk_score_.v0",
        "type": "OUTPUT",
        "elements": {
          "gt0030": {
            "id": "gt0030",
            "path": "/data[at0001]/items[at0002]"
          }
        }
      }
    },
    "rules": {
      "gt0031": {
        "id": "gt0031",
        "priority": 7,
        "when": [
          "$gt0019|Previous VTE|!=null",
          "$gt0020|Known thrombophilia|!=null",
          "$gt0021|Current lower-limb paralysis|!=null",
          "$gt0022|Current cancer|!=null",
          "$gt0023|Immobilized ≥7 days*|!=null",
          "$gt0024|ICU/CCU stay|!=null",
          "$gt0025|Age >60 years|!=null",
          "$gt0026|D-dimer ≥2x ULN|!=null"
        ],
        "then": [
          "$gt0028|Total score|.magnitude=$gt0019.value+$gt0020.value+$gt0021.value+$gt0022.value+$gt0023.value+$gt0024.value+$gt0025.value+$gt0026.value"
        ]
      },
      "gt0032": {
        "id": "gt0032",
        "priority": 6,
        "when": [
          "$gt0028|Total score|==0"
        ],
        "then": [
          "$gt0030|Score Interpretation|=0|local::at0003|0.4 % 42-day VTE risk and 0.5 % 77-day VTE risk|"
        ]
      },
      "gt0033": {
        "id": "gt0033",
        "priority": 5,
        "when": [
          "$gt0028|Total score|==1"
        ],
        "then": [
          "$gt0030|Score Interpretation|=1|local::at0004|0.6 % 42-day VTE risk and 0.7 % 77-day VTE risk|"
        ]
      },
      "gt0034": {
        "id": "gt0034",
        "priority": 4,
        "when": [
          "$gt0028|Total score|==2"
        ],
        "then": [
          "$gt0030|Score Interpretation|=2|local::at0005|0.8 % 42-day VTE risk and 1.0 % 77-day VTE risk|"
        ]
      },
      "gt0035": {
        "id": "gt0035",
        "priority": 3,
        "when": [
          "$gt0028|Total score|==3"
        ],
        "then": [
          "$gt0030|Score Interpretation|=3|local::at0006|1.2 % 42-day VTE risk and 1.4 % 77-day VTE risk|"
        ]
      },
      "gt0036": {
        "id": "gt0036",
        "priority": 2,
        "when": [
          "$gt0028|Total score|==4"
        ],
        "then": [
          "$gt0030|Score Interpretation|=4|local::at0007|1.6 % 42-day VTE risk and 1.9 % 77-day VTE risk|"
        ]
      },
      "gt0037": {
        "id": "gt0037",
        "priority": 1,
        "when": [
          "$gt0028|Total score|>=5"
        ],
        "then": [
          "$gt0030|Score Interpretation|=5|local::at0008|2.2 % 42-day VTE risk and 2.7 % 77-day VTE risk|"
        ]
      }
    }
  },
