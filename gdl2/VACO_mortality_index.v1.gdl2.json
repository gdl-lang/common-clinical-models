  "ontology": {
    "term_definitions": {
      "en": {
        "id": "en",
        "terms": {
          "gt0001": {
            "id": "gt0001",
            "text": "Veterans Health Administration COVID-19 (VACO) Index for COVID-19 Mortality",
            "description": "An assessment index used to estimate the risk of 30-day mortality after COVID-19 infection using pre-COVID health status."
          },
          "gt0003": {
            "id": "gt0003",
            "text": "Age",
            "description": "Age in years, and for babies: months, weeks or days"
          },
          "gt0004": {
            "id": "gt0004",
            "text": "Gender",
            "description": "*"
          },
          "gt0006": {
            "id": "gt0006",
            "text": "AIDS",
            "description": "AIDS (Acquired Immunodeficiency Syndrome). ICD-10 Diagnosis Codes: B20.xâB22.x, B24.x."
          },
          "gt0007": {
