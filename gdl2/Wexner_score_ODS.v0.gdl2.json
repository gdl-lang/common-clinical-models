    "rules": {
      "gt0012": {
        "id": "gt0012",
        "priority": 3,
        "when": [
          "$gt0003|Incontinence to solid stool|!=null",
          "$gt0004|Incontinence to liquid stool|!=null",
          "$gt0005|Incontinence to gas|!=null",
          "$gt0006|Wears pad|!=null",
          "$gt0007|Lifestyle alteration|!=null"
        ],
        "then": [
          "$gt0009|Wexner score|.magnitude=$gt0007.value+$gt0006.value+$gt0005.value+$gt0004.value+$gt0003.value"
        ]
      },
      "gt0016": {
        "id": "gt0016",
        "priority": 2,
        "when": [
          "$gt0009|Wexner score|==20"
        ],
        "then": [
          "$gt0011|Interpretation|.value=$gt0015.term"
        ]
      },
      "gt0013": {
        "id": "gt0013",
        "priority": 1,
        "when": [
          "$gt0009|Wexner score|==0"
        ],
        "then": [
          "$gt0011|Interpretation|.value=$gt0014.term"
        ]
      }
    }
