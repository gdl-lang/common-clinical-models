	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the results for each component parameter calculation and grade for the Implant Disease Risk Assessment (IDRA) score.">
			use = <"Use to record the results for each component parameter calculation and grade for the Implant Disease Risk Assessment (IDRA) score.">
			misuse = <"This IDRA risk assessment tool will require validation through retrospective or prospective studies in multiple private practice and University settings.
This is to guide considerations about care and should be used in conjunction with comprehensive treatment planning.">
			copyright = <"">
		>
	>
