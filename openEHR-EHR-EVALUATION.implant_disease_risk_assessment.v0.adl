definition
	EVALUATION[at0000] matches {	-- Implant disease risk assessment
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- History of Periodontitis Low
						value matches {
							0|[local::at0003], 	-- No
							1|[local::at0005]  	-- Yes
						}
					}
					ELEMENT[at0054] occurrences matches {0..1} matches {	-- History of Periodontitis High
						value matches {
							0|[local::at0070], 	-- No
							1|[local::at0071]  	-- Yes
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Bleeding on Probing Low
						value matches {
							0|[local::at0010], 	-- No
							1|[local::at0011]  	-- Yes
						}
					}
					ELEMENT[at0055] occurrences matches {0..1} matches {	-- Bleeding on Probing Moderate
						value matches {
							0|[local::at0072], 	-- No
							1|[local::at0073]  	-- Yes
