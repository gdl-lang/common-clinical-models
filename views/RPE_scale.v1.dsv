<body ng-controller="cds_controller">
<#assign app_info = "def.guides['Rated perceived exertion scale.v1'].concept">
<#assign references = "def.terms['vt0001']"/>
<#assign rated_percieved_exertion_scale_entry_cds_data = "data.cds['openEHR-EHR-OBSERVATION.rated_perceived_exertion_scale.v1']">
<#assign rated_percieved_exertion_scale_entry_ehr_data = "data.ehr['openEHR-EHR-OBSERVATION.rated_perceived_exertion_scale.v1']">
<#assign rated_percieved_exertion_scale_entry_def = "def.entries['openEHR-EHR-OBSERVATION.rated_perceived_exertion_scale.v1']">
<#assign rpe_scale_level_def = rated_percieved_exertion_scale_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign final_score_def = rated_percieved_exertion_scale_entry_def + ".elements['/data{at0001}/events{at0002}/data{at0003}/items{at0017}']">
<#assign event_time_def = rated_percieved_exertion_scale_entry_def + ".elements['/data/events/time']">
<#assign rpe_scale_level_cds_data = rated_percieved_exertion_scale_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign final_score_cds_data = rated_percieved_exertion_scale_entry_cds_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0017}']">
<#assign event_time_cds_data = rated_percieved_exertion_scale_entry_cds_data + "[0].elements['/data/events/time']">
<#assign rpe_scale_level_ehr_data = rated_percieved_exertion_scale_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0004}']">
<#assign final_score_ehr_data = rated_percieved_exertion_scale_entry_ehr_data + "[0].elements['/data{at0001}/events{at0002}/data{at0003}/items{at0017}']">
<#assign event_time_ehr_data = rated_percieved_exertion_scale_entry_ehr_data + "[0].elements['/data/events/time']">

<script> 
angular.module('cds_app').controller('RPE_scale_Ctrl', function($scope, $window){
  $scope.$on('json_loaded', function(){
    $scope.${event_time_ehr_data}.value.value = new Date().toISOString();
	getInformation();
    $scope.${rpe_scale_level_ehr_data}.value.selection = $scope.findMin($scope.${rpe_scale_level_def});
  });
});

function getInformation() {
    var x = document.getElementById('infobox');
    if (x.style.display === 'none') {
        x.style.display = 'block';
    } else {
        x.style.display = 'none';
    }

}

</script>

<div ng-show="ready" ng-controller="RPE_scale_Ctrl">
<div ng-show="ready">
      <h1>{{def.concept.name}}</h1>
      <hr style="height:1.5px;border:none;color:SlateGray;background-color:SlateGray;" />
        <div class="wrapper1">
          <button class="button" onclick="getInformation()">App Description</button>
          <div id="infobox">
            <h2>{{def.concept.description}}</h2>
            <br />
            <div id="geninfo">
              {{${app_info}.description}}
            </div>
          </div>
        </div>
  </div>
  
  <div class="card">
<div class="row collapse" title="{{${rpe_scale_level_def}.description}}">
    <div class="medium-5 columns">
      <label>{{${rpe_scale_level_def}.name}}</label>
      </div>
      <label class="medium-7 columns">
        <select ng-model="${rpe_scale_level_ehr_data}.value.selection" ng-options="key as (key.charAt(0) + ' - ' + desc) for (key, desc) in ${rpe_scale_level_def}.selections" ng-change="execute()" ng-model-options="{ updateOn: 'default blur', debounce: { default: 500, blur: 0 } }">
        </select>
      </label>
    </div>
<div class="displayresult2" title="{{${rpe_scale_level_def}.description}}">
      <div class="row collapse">
        <div class="medium-6 columns">
          <label><b>{{${rpe_scale_level_def}.name}}: </b></label>
        </div>
        <div class="medium-6 columns" style="text-align: center;">
          <label>{{${rpe_scale_level_cds_data}.value.symbol.value}}</label>
        </div>
      </div>
    </div>
  </div>
</div>

<br /><br />

<div class="references1">
  <div class="row collapse">
    <div class="medium-12 columns">
      <h3>{{${references}.text}}</h3>
        <div id="ref">
          <li>
            <i><a href="https://my.clevelandclinic.org/health/articles/rpe-scale-heart-health?view=print">Rated Perceived Exertion Scale taken from the Cleveland Clinic website, published 3rd March 2017. Accessed at: https://my.clevelandclinic.org/health/articles/rpe-scale-heart-health?view=print</a>
            </i>
          </li>
        </div>
    </div>
  </div>
</div>

<br />

