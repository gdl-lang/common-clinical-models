	ontology = (GUIDE_ONTOLOGY) <
		term_definitions = <
			["en"] = (TERM_DEFINITION) <
				terms = <
					["gt0001"] = (TERM) <
						text = <"International Prostate Symptom Score (IPSS)">
						description = <"The International Prostate Symptom Score (IPSS) is used to assist in the rapid diagnosis of Benign Prostatic Hyperplasia (BPH), as well as monitor the symptoms of the disease over time. The questionnaire consists first of 7 questions addressing how the patient has experienced the following symptoms over the past month: incomplete emptying (of the bladder), frequency, intermittency, urgency, weak stream, straining, and nocturia. The responses are scored from 0 (no symptoms) to 5 (worst possible) and a score of ≤7 indicates mild symptoms, 8-19 moderate symptoms, and 20-35 severe symptoms. An 8th question which addresses the perceived quality of life as a result of the urinary symptoms, does not contribute to the overall score or its assessment.
">
					>
					["gt0005"] = (TERM) <
						text = <"Incomplete emptying over the past month">
						description = <"Sensation of incomplete emptying.">
					>
					["gt0006"] = (TERM) <
						text = <"Frequency over the past month">
						description = <"Frequency of urination.">
					>
					["gt0007"] = (TERM) <
						text = <"Intermittency over the past month">
						description = <"Intermittency of urination.">
					>
					["gt0008"] = (TERM) <
						text = <"Urgency over the past month">
						description = <"Feeling of urgency.">
					>
					["gt0009"] = (TERM) <
						text = <"Weak urinary stream over the past month">
						description = <"Weak stream of urine.">
					>
					["gt0010"] = (TERM) <
						text = <"Straining over the past month">
						description = <"Need to strain in order to start urination.">
					>
					["gt0011"] = (TERM) <
						text = <"Nocturia over the past month">
						description = <"Frequency of nocturia.">
					>
					["gt0012"] = (TERM) <
						text = <"If you were to spend the rest of your life with your urinary condition just the way it is not, how would you feel about that?">
						description = <"Additional question, not included in total score.">
					>
					["gt0022"] = (TERM) <
						text = <"Total score">
						description = <"Sum of all factors. Last question not included.">
					>
					["gt0032"] = (TERM) <
						text = <"Calculate total score">
					>
					["gt0035"] = (TERM) <
						text = <"Perceived quality of life with urinary symptoms">
						description = <"Additional question, not included in total score.">
					>
					["gt0036"] = (TERM) <
