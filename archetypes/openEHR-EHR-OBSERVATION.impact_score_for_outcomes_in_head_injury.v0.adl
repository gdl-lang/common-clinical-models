										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {	-- Core score 
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::380]>
												list = <
													["1"] = <
														units = <"1">
													>
												>
											>
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {	-- Hypoxia
										value matches {
											0|[local::at0023], 	-- No
											1|[local::at0024]  	-- Yes or suspected
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {	-- Hypotension
