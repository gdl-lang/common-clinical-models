											5|[local::at0010]  	-- >=70
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {	-- Motor Score
										value matches {
											0|[local::at0012], 	-- Localized/ obeys
											2|[local::at0013], 	-- Normal flexion
											3|[local::at0014], 	-- Untestable/ missing
											4|[local::at0015], 	-- Abnormal flexion
											6|[local::at0016]  	-- None/ extension
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Pupillary reactivity
