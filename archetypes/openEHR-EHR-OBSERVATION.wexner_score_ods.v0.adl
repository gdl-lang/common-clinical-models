definition
	OBSERVATION[at0000] matches {    -- Wexner score ODS
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {0..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {    -- Assesment
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0005] occurrences matches {0..1} matches {    -- Incontinence to solid stool
												value matches {
													0|[local::at0012],
													1|[local::at0013],
													2|[local::at0014],
													3|[local::at0015],
													4|[local::at0016]
												}
											}
											ELEMENT[at0006] occurrences matches {0..1} matches {    -- Incontinence to liquid stool
												value matches {
													0|[local::at0017],
													1|[local::at0018],
													2|[local::at0019],
													3|[local::at0020],
													4|[local::at0021]
												}
											}
											ELEMENT[at0007] occurrences matches {0..1} matches {    -- Incontinence to gas
												value matches {
													0|[local::at0022],
													1|[local::at0023],
													2|[local::at0024],
													3|[local::at0025],
													4|[local::at0026]
												}
											}
											ELEMENT[at0010] occurrences matches {0..1} matches {    -- Wears pad
												value matches {
													0|[local::at0027],
													1|[local::at0028],
													2|[local::at0029],
													3|[local::at0030],
													4|[local::at0031]
												}
											}
											ELEMENT[at0011] occurrences matches {0..1} matches {    -- Lifestyle alteration
												value matches {
													0|[local::at0032],
													1|[local::at0033],
													2|[local::at0034],
													3|[local::at0035],
													4|[local::at0036]
												}
											}
										}
									}
									ELEMENT[at0039] occurrences matches {0..1} matches {    -- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..20|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {*}    -- Item tree
		}
	}

