					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..4} matches {	-- شعرت بالدوار أو الدوخة أو الإغماء عندما قرأت أو استمعت إلى أخبار عن فيروس كورونا
										value matches {
											0|[local::at0006], 	-- على الاطلاق
											1|[local::at0007], 	-- نادر ، أقل من يوم أو يومين
											2|[local::at0008], 	-- عدة أيام
											3|[local::at0009], 	-- أكثر من 7 أيام
											4|[local::at0010]; 	-- تقريبًا كل يوم على مدار الأسبوعين الماضيين
											0  	-- assumed value
										}
									}
									ELEMENT[at0012] occurrences matches {0..4} matches {	-- واجهت مشكلة في النوم أو البقاء نائماً لأنني كنت أفكر في فيروس كورونا
										value matches {
											0|[local::at0013], 	-- على الاطلاق
											1|[local::at0014], 	-- نادر ، أقل من يوم أو يومين
											2|[local::at0015], 	-- عدة أيام
											3|[local::at0016], 	-- أكثر من 7 أيام
											4|[local::at0017]; 	-- تقريبًا كل يوم على مدار الأسبوعين الماضيين
