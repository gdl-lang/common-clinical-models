										}
									}
									ELEMENT[at0018] occurrences matches {0..4} matches {	-- شعرت بالشلل أو التجمد عندما فكرت أو تعرضت لمعلومات عن فيروس كورونا
										value matches {
											0|[local::at0031], 	-- على الاطلاق
											1|[local::at0032], 	-- نادر ، أقل من يوم أو يومين
											2|[local::at0033], 	-- عدة أيام
											3|[local::at0034], 	-- أكثر من 7 أيام
											4|[local::at0035]; 	-- تقريبًا كل يوم على مدار الأسبوعين الماضيين
											0  	-- assumed value
										}
									}
									ELEMENT[at0019] occurrences matches {0..4} matches {	-- فقدت الاهتمام بتناول الطعام عندما فكرت أو تعرضت لمعلومات عن فيروس كورونا
										value matches {
											0|[local::at0026], 	-- على الاطلاق
											1|[local::at0027], 	-- نادر ، أقل من يوم أو يومين
											2|[local::at0028], 	-- عدة أيام
											3|[local::at0029], 	-- أكثر من 7 أيام
											4|[local::at0030]; 	-- تقريبًا كل يوم على مدار الأسبوعين الماضيين
											0  	-- assumed value
										}
									}
									ELEMENT[at0020] occurrences matches {0..4} matches {	-- شعرت بالغثيان أو كنت أعاني من مشاكل في المعدة عندما فكرت أو تعرضت لمعلومات عن فيروس كورونا
										value matches {
											0|[local::at0021], 	-- على الاطلاق
											1|[local::at0022], 	-- نادر ، أقل من يوم أو يومين
											2|[local::at0023], 	-- عدة أيام
											3|[local::at0024], 	-- أكثر من 7 أيام
											4|[local::at0025]; 	-- تقريبًا كل يوم على مدار الأسبوعين الماضيين
											0  	-- assumed value
										}
									}
									ELEMENT[at0036] occurrences matches {0..20} matches {	-- مجموع النقاط
										value matches {
											DV_COUNT matches {
												magnitude matches {|0..20|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

