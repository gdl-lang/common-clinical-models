definition
	OBSERVATION[at0000.1] matches {	-- Leverprover
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(en)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- *Test name(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0077] occurrences matches {0..1} matches {	-- *Diagnostic service(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0073] occurrences matches {0..1} matches {	-- *Test status(en)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0037, 	-- *Interim(en)
													at0038, 	-- *Final(en)
													at0039, 	-- *Supplementary(en)
													at0040, 	-- *Corrected (amended)(en)
													at0074, 	-- *Aborted(en)
													at0079]	-- *Never performed(en)
												}
											}
										}
									}
									allow_archetype CLUSTER[at0065] occurrences matches {0..*} matches {	-- *Specimen detail(en)
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.specimen\.v1/}
										exclude
											archetype_id/value matches {/.*/}
									}
									ELEMENT[at0078.2] occurrences matches {0..1} matches {	-- Alkaliskt fosfatas (ALP)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"U/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.4] occurrences matches {0..1} matches {	-- Bilirubin - totalt
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"µmol/l">
														magnitude = <|>=0.0|>
													>
													["2"] = <
														units = <"mg/dl">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.11] occurrences matches {0..1} matches {	-- Bilirubin - konjugerat
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"µmol/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.9] occurrences matches {0..1} matches {	-- Bilirubin - okonjugerat
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"µmol/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.1] occurrences matches {0..1} matches {	-- Alaninaminotransferas (ALAT)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"U/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.3] occurrences matches {0..1} matches {	-- Aspartataminotransferas (ASAT)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"U/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.5] occurrences matches {0..1} matches {	-- Gamma-glutamyltransferas (gamma-GT)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"U/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.6] occurrences matches {0..1} matches {	-- Laktatdehydrogenas (LD)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"U/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.7] occurrences matches {0..1} matches {	-- Albumin
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"gm/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.8] occurrences matches {0..1} matches {	-- *Globulins(en)
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"gm/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									ELEMENT[at0078.10] occurrences matches {0..1} matches {	-- Totalt protein
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::119]>
												list = <
													["1"] = <
														units = <"gm/l">
														magnitude = <|>=0.0|>
													>
												>
											>
										}
									}
									allow_archetype CLUSTER[at0089] occurrences matches {0..*} matches {	-- *Per-result annotation(en)
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.lab_result_annotation(-[a-zA-Z0-9_]+)*\.v1/}
										exclude
											archetype_id/value matches {/.*/}
									}
									ELEMENT[at0057] occurrences matches {0..1} matches {	-- *Overall interpretation(en)
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0010] occurrences matches {0..*} matches {	-- *Multimedia representation(en)
										value matches {
											DV_MULTIMEDIA matches {
												media_type matches {[openEHR::]}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {	-- *Tree(en)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0062] occurrences matches {0..1} matches {	-- *Requestor order identifier(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0090] occurrences matches {0..*} matches {	-- *Requestor (en)
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0063] occurrences matches {0..1} matches {	-- *Receiver order Identifier(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0017] occurrences matches {0..*} matches {	-- *Receiving laboratory (en)
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0068] occurrences matches {0..1} matches {	-- *Laboratory test result identifier(en)
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0075] occurrences matches {0..1} matches {	-- *Datetime result issued(en)
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}

