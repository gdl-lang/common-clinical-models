definition
	OBSERVATION[at0000] matches {	-- Sjukdomshistoria/tidigare sjukdomar
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(en)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *Any event(en)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Tree(en)
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Kardiovaskulär sjukdom
										value matches {
											0|[local::at0024], 	-- *No(en)
											1|[local::at0025]  	-- *Yes(en)
										}
									}
									ELEMENT[at0017] occurrences matches {0..1} matches {	-- Lung-/luftvägssjukdom
										value matches {
											0|[local::at0027], 	-- *No(en)
											1|[local::at0028]  	-- *Yes(en)
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {	-- Reumatisk/annan autoimmun sjukdom
