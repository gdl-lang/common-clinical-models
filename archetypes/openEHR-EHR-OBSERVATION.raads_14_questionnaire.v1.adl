	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Ritvo Autism and Asperger Diagnostic Scale 14 (RAADS-14) - questionnaire">
					description = <"Screening tool for autism spectrum disorder (ASD) in an adult psychiatric population. Consists of 14 self-administered questions derived from the 80-item Ritvo Autism and Asperger Diagnostic Scale-Revised (RAADS-R) and covers three domains: mentalizing deficits, social anxiety and sensory reactivity. Each question is scored 0, 1, 2 or 3 and the total score ranges from a minimum of 0 to a maximum of 42. The total score is assessed as: ≥31 = autism very likely; 14 – 30 = autism likely; 0 – 13 No autism symptoms (autism unlikely).">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"*">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Mentalizing Deficits">
					description = <"*">
				>
				["at0007"] = <
					text = <"It is difficult for me to understand how other people are feeling when we are talking.">
					description = <"*">
				>
				["at0012"] = <
					text = <"It is difficult to figure out what other people expect of me.">
					description = <"*">
				>
				["at0016"] = <
					text = <"When talking to someone, I have a hard time telling when it is my turn to talk or to listen.">
					description = <"*">
				>
				["at0021"] = <
					text = <"It can be very hard to read someone’s face, hand, and body movements when we are talking.">
					description = <"*">
				>
				["at0026"] = <
					text = <"I focus on details rather than the overall idea.">
					description = <"*">
				>
				["at0032"] = <
					text = <"I take things too literally, so I often miss what people are trying to say.">
					description = <"*">
				>
				["at0036"] = <
					text = <"I get extremely upset when the way I like to do things is suddenly changed.">
					description = <"*">
				>
				["at0041"] = <
					text = <"Social Anxiety">
					description = <"*">
				>
				["at0043"] = <
					text = <"It is very difficult for me to work and function in groups.">
					description = <"*">
