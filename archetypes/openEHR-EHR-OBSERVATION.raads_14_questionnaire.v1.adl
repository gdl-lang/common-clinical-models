definition
	OBSERVATION[at0000] matches {	-- Ritvo Autism and Asperger Diagnostic Scale 14 (RAADS-14) - questionnaire
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Mentalizing Deficits
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- It is difficult for me to understand how other people are feeling when we are talking.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- It is difficult to figure out what other people expect of me.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0016] occurrences matches {0..1} matches {	-- When talking to someone, I have a hard time telling when it is my turn to talk or to listen.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0021] occurrences matches {0..1} matches {	-- It can be very hard to read someone’s face, hand, and body movements when we are talking.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0026] occurrences matches {0..1} matches {	-- I focus on details rather than the overall idea.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0032] occurrences matches {0..1} matches {	-- I take things too literally, so I often miss what people are trying to say.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0036] occurrences matches {0..1} matches {	-- I get extremely upset when the way I like to do things is suddenly changed.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
										}
									}
									CLUSTER[at0041] occurrences matches {0..1} matches {	-- Social Anxiety
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0043] occurrences matches {0..1} matches {	-- It is very difficult for me to work and function in groups.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0047] occurrences matches {0..1} matches {	-- I often don’t know how to act in social situations.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0052] occurrences matches {0..1} matches {	-- I can chat and make small talk with people.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0057] occurrences matches {0..1} matches {	-- How to make friends and socialize is a mystery to me.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
										}
									}
									CLUSTER[at0063] occurrences matches {0..1} matches {	-- Sensory Reactivity
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0064] occurrences matches {0..1} matches {	-- Some ordinary textures that do not bother others feel very offensive when they touch my skin.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0068] occurrences matches {0..1} matches {	-- When I feel overwhelmed by my senses, I have to isolate myself to shut them down.
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
											ELEMENT[at0073] occurrences matches {0..1} matches {	-- Sometimes I have to cover my ears to block out painful noises (like vacuum cleaners or people talking too much or too loudly).
												value matches {
													0|[local::at0137], 	-- Never true
													1|[local::at0138], 	-- True only when I was younger than 16 years
													2|[local::at0139], 	-- True only now
													3|[local::at0140]  	-- True now and when I was young
												}
											}
										}
									}
									ELEMENT[at0079] occurrences matches {0..1} matches {	-- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|>=0|}
											}
										}
									}
									ELEMENT[at0080] occurrences matches {0..1} matches {	-- Comment
										value matches {
