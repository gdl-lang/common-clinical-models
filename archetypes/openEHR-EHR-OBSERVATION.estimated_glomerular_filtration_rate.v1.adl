definition
	OBSERVATION[at0000] matches {	-- Estimated Glomerular Filtration Rate
		data matches {
			HISTORY[at0001] matches {	-- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {1..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Single
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0004] occurrences matches {1..2} matches {	-- Estimated Glomerular Filtration Rate
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::126]>
												list = <
													["1"] = <
														units = <"ml/min">
													>
												>
											>
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {	-- Estimated Glomerular Filtration Rate - Relative
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::586]>
												list = <
													["1"] = <
														units = <"ml/min/1.73m2">
													>
												>
											>
										}
									}
									ELEMENT[at0022] occurrences matches {0..1} matches {	-- Formula
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0023, 	-- Cockcroft-Gault formula
													at0024, 	-- MDRD formula
													at0029, 	-- Revised Lund-Malmö formula
													at0028, 	-- Revised Schwartz formula (Age group 1-16 years)
													at0025, 	-- CKD-EPI Creatinine formula
													at0031, 	-- CKD-EPI Cystatin C formula
													at0032]	-- CKD-EPI Creatinine Cystatin C formula
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_LIST[at0005] matches {	-- List
				items cardinality matches {0..*; ordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007, 	-- Automatic entry
									at0008]	-- Direct entry
								}
							}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {	-- Formula
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0013, 	-- MDRD formula - non-IDMS-normalized serum Cr
									at0014, 	-- MDRD formula - IDMS-normalized serum Cr
									at0015, 	-- CKD-EPI formula
									at0017, 	-- Bedside Schwartz equation
									at0016, 	-- Cockcroft-Gault formula
									at0019, 	-- Other formula
									at0021]	-- LM Revised
								}
							}
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

