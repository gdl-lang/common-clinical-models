	details = <
		["da"] = <
			language = <[ISO_639-1::da]>
			purpose = <"*To record the Estimated Glomerular Filtration Rate (eGFR) of a person.
Estimated Glomerular Filtration Rate is an overall measure of kidney function. It is used to facilitate the detection, evaluation and management of chronic kidney disease.(en)">
			use = <"*Use to record the Estimated Glomerular Filtration Rate of children (Schwartz equation) and adults between 18 and 110 years.  
Use to enter the Estimated Glomerular Filtration Rate either manually (i.e. calculated and directly entered by the clinician), or automatically (ie calculation and entry is done automatically by a software application, based on separate variables measurements).(en)">
			keywords = <"*kidney function(en)", "*glomerular filtration rate(en)", "*GFR(en)", "*creatinine(en)", "*kidney(en)", "*chronic kidney disease(en)", "*CKD-EPI formula(en)", "*MDRD formula(en)", "*Schwartz equation(en)">
			misuse = <"*Not intended to record information regarding creatinine values - these will be recorded in separate archetypes.(en)">
			copyright = <"*© 2011 openEHR Foundation(en)">
		>
		["el"] = <
