	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the results for each component parameter and the calculated SLEDAI-2K.">
			use = <"Use to record the results for each component parameter and the calculated SLEDAI-2K in order to stratify severity of systemic lupus erythematosus (SLE).">
			keywords = <"systemic lupus erythematosus disease activity index 2000", "SLEDAI-2K", "seizure", "pscyhosis", "organic brain syndrome", "visual disturbance", "lupus headache", "stroke", "vasculitis", "arthritis", "myositis", "hematuria", "proteinuria", "pyuria", "inflammatory-type rash", "alopecia", "oral ulcers", "pericarditis", "temp", "platelets", "WBC">
			misuse = <"">
			copyright = <"Cambio CDS">
		>
	>
