definition
	EVALUATION[at0000] matches {	-- Systemic Lupus Erythematosus Disease Activity Index 2000 (SLEDAI-2K)
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Recent onset seizure
						value matches {
							0|[local::at0003], 	-- No
							8|[local::at0004]  	-- Yes
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Psychosis
						value matches {
							0|[local::at0006], 	-- No
							8|[local::at0007]  	-- Yes
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Organic brain syndrome
						value matches {
							0|[local::at0009], 	-- No
							8|[local::at0010]  	-- Yes
						}
					}
					ELEMENT[at0011] occurrences matches {0..1} matches {	-- Visual disturbance
						value matches {
							0|[local::at0012], 	-- No
							8|[local::at0013]  	-- Yes
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- New onset sensory or motor neuropathy involving cranial nerves
						value matches {
							0|[local::at0015], 	-- No
							8|[local::at0016]  	-- Yes
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Lupus headache
						value matches {
							0|[local::at0018], 	-- No
							8|[local::at0019]  	-- Yes
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- New onset stroke
						value matches {
							0|[local::at0023], 	-- No
							8|[local::at0024]  	-- Yes
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {	-- Vasculitis
						value matches {
							0|[local::at0025], 	-- No
							8|[local::at0026]  	-- Yes
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Arthritis
						value matches {
							0|[local::at0027], 	-- No
							4|[local::at0028]  	-- Yes
						}
					}
					ELEMENT[at0029] occurrences matches {0..1} matches {	-- Myositis
						value matches {
							0|[local::at0030], 	-- No
							4|[local::at0031]  	-- Yes
						}
					}
					ELEMENT[at0032] occurrences matches {0..1} matches {	-- Heme-granular or RBC urinary casts
						value matches {
							0|[local::at0033], 	-- No
							4|[local::at0034]  	-- Yes
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {	-- Hematuria
						value matches {
							0|[local::at0036], 	-- No
							4|[local::at0037]  	-- Yes
						}
					}
					ELEMENT[at0038] occurrences matches {0..1} matches {	-- Proteinuria
						value matches {
							0|[local::at0039], 	-- No
							4|[local::at0040]  	-- Yes
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {	-- Pyuria
						value matches {
							0|[local::at0045], 	-- No
							2|[local::at0046]  	-- Yes
						}
					}
					ELEMENT[at0042] occurrences matches {0..1} matches {	-- Inflammatory-type rash
						value matches {
							0|[local::at0043], 	-- No
							2|[local::at0044]  	-- Yes
						}
					}
					ELEMENT[at0047] occurrences matches {0..1} matches {	-- Alopecia
						value matches {
							0|[local::at0048], 	-- No
							2|[local::at0049]  	-- Yes
						}
					}
					ELEMENT[at0050] occurrences matches {0..1} matches {	-- Oral or nasal mucosal ulcers
						value matches {
							0|[local::at0051], 	-- No
							2|[local::at0052]  	-- Yes
						}
					}
					ELEMENT[at0053] occurrences matches {0..1} matches {	-- Pleuritic chest pain with pleural rub/effusion or pleural thickening
						value matches {
							0|[local::at0054], 	-- No
							2|[local::at0055]  	-- Yes
						}
					}
					ELEMENT[at0056] occurrences matches {0..1} matches {	-- Pericarditis
						value matches {
							0|[local::at0060], 	-- No
							2|[local::at0061]  	-- Yes
						}
					}
					ELEMENT[at0057] occurrences matches {0..1} matches {	-- Low complement
						value matches {
							0|[local::at0058], 	-- No
							2|[local::at0059]  	-- Yes
						}
					}
					ELEMENT[at0062] occurrences matches {0..1} matches {	-- High DNA binding
						value matches {
							0|[local::at0063], 	-- No
							2|[local::at0064]  	-- Yes
						}
					}
					ELEMENT[at0065] occurrences matches {0..1} matches {	-- Temp >100.4 °F (38°C)
						value matches {
							0|[local::at0066], 	-- No
							1|[local::at0067]  	-- Yes
						}
					}
					ELEMENT[at0068] occurrences matches {0..1} matches {	-- Platelets <100 x 109/L
						value matches {
							0|[local::at0072], 	-- No
							1|[local::at0073]  	-- Yes
						}
					}
					ELEMENT[at0069] occurrences matches {0..1} matches {	-- WBC <3 x 109/L
						value matches {
							0|[local::at0070], 	-- No
							1|[local::at0071]  	-- Yes
						}
					}
					ELEMENT[at0074] occurrences matches {0..1} matches {	-- Total score
						value matches {
							DV_COUNT matches {
								magnitude matches {|0..105|}
							}
						}
					}
					ELEMENT[at0075] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

