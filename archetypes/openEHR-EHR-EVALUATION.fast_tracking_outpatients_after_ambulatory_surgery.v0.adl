archetype (adl_version=1.4)
	openEHR-EHR-EVALUATION.fast_tracking_outpatients_after_ambulatory_surgery.v0

concept
	[at0000]	-- Fast tracking outpatients after ambulatory surgery
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"A quick tracking of some signals and symptoms in order to perform a shorter postoperative recovery process">
			use = <"Can be use by different healthcare professionals that are responsible for the patient in order to decide if the patient can be discharged from the unit area.">
			keywords = <"Fast-tracking", "Ambulatory Surgery", "Discharge", "Score">
			misuse = <"The fast-Tracking scale after ambulatory surgery is a complement of a complet assessment that the responsible caregiver needs to perform in order to valorate if the patient is ready to go home, but in any case can be a substitutive of the assessment. ">
			copyright = <"@CambioCDS">
		>
	>
	lifecycle_state = <"0">
	other_contributors = <>
	other_details = <
		["references"] = <"1.         Watkins AC, White PF. Fast-tracking after ambulatory surgery. Journal of Perianesthesia Nursing. 2001;16(6):379–87. 
2.         White PF. Criteria for fast-tracking outpatients after ambulatory surgery. J Clin Anesth. 1999 Feb;11(1):78-9. doi: 10.1016/s0952-8180(98)00119-6. PMID: 10396724
 
">
		["MD5-CAM-1.0.1"] = <"18BDF74F549E6FF376314420B7464F31">
	>

definition
	EVALUATION[at0000] matches {	-- Fast tracking outpatients after ambulatory surgery
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Level of consciousness
						value matches {
							0|[local::at0034], 	-- Responsive only to tactile stimulation
							1|[local::at0033], 	-- Arousable with minimal stimulation
							2|[local::at0032]  	-- Awake and Oriented
						}
					}
					ELEMENT[at0035] occurrences matches {0..1} matches {	-- Physical activity
						value matches {
							0|[local::at0036], 	-- Unable to voluntary move extremities
							1|[local::at0037], 	-- Some weakness in movement of extremities
							2|[local::at0038]  	-- Able to move all extremities on command
						}
					}
					ELEMENT[at0039] occurrences matches {0..1} matches {	-- Hemodynamic stability
						value matches {
							0|[local::at0043], 	-- Blood pressure ± 50% of baseline
							1|[local::at0041], 	-- Blood pressure ± 30 % of baseline
							2|[local::at0040]  	-- Blood pressure ± 15% of baseline
						}
					}
					ELEMENT[at0044] occurrences matches {0..1} matches {	-- Respiratory stability
						value matches {
							0|[local::at0048], 	-- Dyspneic with weak cough
							1|[local::at0046], 	-- Tachypnea with good cough
							2|[local::at0045]  	-- Respiratory rate 10 to 20 breaths/min, able to breathe deeply
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {	-- Oxygen saturation status
						value matches {
							0|[local::at0050], 	-- saturation <90% with supplemental oxygen
							1|[local::at0051], 	-- requires supplemental oxygen to maintain saturation
							2|[local::at0052]  	-- maintains value >90% while breathing room air
						}
					}
					ELEMENT[at0053] occurrences matches {0..1} matches {	-- Postoperative pain assessment
						value matches {
							0|[local::at0054], 	-- Persistent moderate-to-severe pain
							1|[local::at0055], 	-- Moderate-to-severe pain controlled with intravenous analgesics
							2|[local::at0056]  	-- No or mild discomfort
						}
					}
					ELEMENT[at0057] occurrences matches {0..1} matches {	-- Postoperative emetic symptoms
						value matches {
							0|[local::at0058], 	-- Persistent moderate-to-severe nausea or vomiting
							1|[local::at0059], 	-- Transient vomiting or retching controlled with intravenous antiemetics
							2|[local::at0060]  	-- No or mild nausea with no active vomiting
						}
					}
					ELEMENT[at0061] occurrences matches {0..1} matches {	-- Fast-track Index Score
						value matches {
							DV_COUNT matches {
								magnitude matches {|0..14|}
							}
						}
					}
					ELEMENT[at0082] occurrences matches {0..1} matches {	-- Score Interpretation: 
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0083, 	-- Patient is elegible to be discharged
									at0084]	-- Patient is not elegible to be discharged
								}
							}
						}
					}
				}
			}
		}
	}

