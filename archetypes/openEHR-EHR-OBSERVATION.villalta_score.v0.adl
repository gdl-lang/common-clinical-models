definition
	OBSERVATION[at0000] matches {	-- *Villalta score(sv)
		data matches {
			HISTORY[at0001] matches {	-- *Event Series(sv)
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- *vilken händelse som helst(sv)
						data matches {
							ITEM_TREE[at0003] matches {	-- *Träd(sv)
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- *Assessment(sv)
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- *Pain(sv)
												value matches {
													0|[local::at0008], 	-- *Absent(sv)
													1|[local::at0009], 	-- *Mild(sv)
													2|[local::at0010], 	-- *Moderate(sv)
													3|[local::at0011]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- *Cramps(sv)
												value matches {
													0|[local::at0013], 	-- *Absent(sv)
													1|[local::at0014], 	-- *Mild(sv)
													2|[local::at0015], 	-- *Moderate(sv)
													3|[local::at0016]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0017] occurrences matches {0..1} matches {	-- *Heaviness(sv)
												value matches {
													0|[local::at0018], 	-- *Absent(sv)
													1|[local::at0019], 	-- *Mild(sv)
													2|[local::at0020], 	-- *Moderate(sv)
													3|[local::at0021]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0022] occurrences matches {0..1} matches {	-- *Parasthesia(sv)
												value matches {
													0|[local::at0023], 	-- *Absent(sv)
													1|[local::at0024], 	-- *Mild(sv)
													2|[local::at0025], 	-- *Moderate(sv)
													3|[local::at0026]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0027] occurrences matches {0..1} matches {	-- *Pruritus(sv)
												value matches {
													0|[local::at0028], 	-- *Absent(sv)
													1|[local::at0029], 	-- *Mild(sv)
													2|[local::at0030], 	-- *Moderate(sv)
													3|[local::at0031]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0032] occurrences matches {0..1} matches {	-- *Pretibial edema(sv)
												value matches {
													0|[local::at0033], 	-- *Absent(sv)
													1|[local::at0034], 	-- *Mild(sv)
													2|[local::at0035], 	-- *Moderate(sv)
													3|[local::at0036]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0037] occurrences matches {0..1} matches {	-- *Skin induration(sv)
												value matches {
													0|[local::at0038], 	-- *Absent(sv)
													1|[local::at0039], 	-- *Mild(sv)
													2|[local::at0040], 	-- *Moderate(sv)
													3|[local::at0041]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0042] occurrences matches {0..1} matches {	-- *Hyperpigmentation(sv)
												value matches {
													0|[local::at0043], 	-- *Absent(sv)
													1|[local::at0044], 	-- *Mild(sv)
													2|[local::at0045], 	-- *Moderate(sv)
													3|[local::at0046]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0047] occurrences matches {0..1} matches {	-- *Redness(sv)
												value matches {
													0|[local::at0048], 	-- *Absent(sv)
													1|[local::at0049], 	-- *Mild(sv)
													2|[local::at0050], 	-- *Moderate(sv)
													3|[local::at0051]  	-- *Severe(sv)
												}
											}
											ELEMENT[at0052] occurrences matches {0..1} matches {	-- *Venous ectasia(sv)
												value matches {
													0|[local::at0053], 	-- *Absent(sv)
													1|[local::at0054], 	-- *Mild(sv)
													2|[local::at0055], 	-- *Moderate(sv)
