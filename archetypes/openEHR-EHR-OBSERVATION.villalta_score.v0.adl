									ELEMENT[at0071] occurrences matches {0..1} matches {	-- *Venous ulcer(sv)
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0072, 	-- *Absent(sv)
													at0073]	-- *Present(sv)
												}
											}
										}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {	-- *Total score(sv)
										value matches {
